{
  "apps": [
    {
      "name": "ipbangui",
      "script": "./node_modules/next/dist/bin/next",
      "args": "start",
      "exec_mode": "cluster",
      "instances": "1",
      "restart_delay": 300000,
      "watch": false,
      "cwd": "./",
      "max_memory_restart": "1512M",
      "out_file": "/dev/null",
      "error_file": "./site.error.log",
      "merge_logs": true
    }
  ]
}
